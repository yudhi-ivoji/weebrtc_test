  "version": "0.2.0",
  "configurations": [
    {
      "name": "voice_call_app (DEV)",
      "cwd": "voice_call_app",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_dev.dart",
      "args": [
        "--dart-define=FLAVOR=development"
      ]
    },
    {
      "name": "voice_call_app (PROD)",
      "cwd": "voice_call_app",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_prod.dart",
      "args": [
        "--dart-define=FLAVOR=production"
      ]
    },
    {
      "name": "voice_call_app (DEV Web)",
      "cwd": "voice_call_app",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_dev.dart",
      "flutterMode": "debug",
      "args": [
        "-d",
        "chrome",
        "--dart-define=FLAVOR=development"
      ]
    },
    {
      "name": "voice_call_app (PROD Web)",
      "cwd": "voice_call_app",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_prod.dart",
      "flutterMode": "debug",
      "args": [
        "-d",
        "chrome",
        "--dart-define=FLAVOR=production"
      ]
    },
    {
      "name": "voice_call_app (profile mode)",
      "cwd": "voice_call_app",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "program": "lib/main_prod.dart",
      "args": [
        "--dart-define=FLAVOR=production"
      ]
    },
    {
      "name": "voice_call_app (release mode)",
      "cwd": "voice_call_app",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "program": "lib/main_prod.dart",
      "args": [
        "--dart-define=FLAVOR=production"
      ]
    }
  ]
